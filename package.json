{
  "name": "templatize-string",
  "displayName": "templatize-string",
  "description": "",
  "version": "0.0.1",
  "publisher": "jansav",
  "engines": {
    "vscode": "^1.27.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:templatize-string.format"
  ],
  "main": "src/index",
  "contributes": {
    "commands": [
      {
        "command": "templatize-string.format",
        "title": "Turn concatenated string to ES6 string template"
      }
    ],
    "keybindings": [
      {
        "command": "templatize-string.format",
        "key": "ctrl+k",
        "mac": "cmd+k",
        "when": "editorTextFocus"
      }
    ]
  },
  "scripts": {
    "postinstall": "node ./node_modules/vscode/bin/install",
    "test": "jest"
  },
  "dependencies": {
    "jest": "^23.6.0",
    "templatize-string": "^1.0.3"
  },
  "devDependencies": {
    "eslint": "^4.11.0",
    "eslint-plugin-jest": "^21.22.1",
    "vscode": "^1.1.21"
  }
}
